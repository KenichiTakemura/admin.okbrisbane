<div class="page-header">
	<h2><%= t("listing_business_categories") %> </h2>
</div>

<div class="row-fluid">
<form class="form-inline" method="post" action="/business_categories/search" id="search-form" data-remote="true" accept-chaset="UTF-8">
<label><%= t("display_name") %></label>
<input name="key" type="text" class="ajax-typeahead" id="key" autocomplete="false">
<input type="submit" class="btn btn-primary" value="<%=t('search')%>">
<%= _script_document_ready(%Q|
$('.ajax-typeahead').typeahead({
    source: function (query, process) {
        return $.get('/business_categories/query', { query: query }, function (data) {
            return process(data);
        });
    }
});
|)%>
</form>
</div>

<div class="row-fluid" id="search_result">
<%= render :partial => 'categories' %>
</div>
